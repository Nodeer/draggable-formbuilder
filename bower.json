{
  "name": "draggable-formbuilder",
  "version": "0.1.0",
  "authors": [
    {
      "name": "Edson A.Soares",
      "email": "edson.araujo.soares@gmail.com"
    }
  ],
  "license": "no",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components"
  ],

  "dependencies" : {
    "bootstrap" : "3.2.0",
    "backbone" : "~1.3.2",
    "backbone.syphon" : "~0.6.3",
    "underscore" : "~1.8.3",
    "text" : "~2.0.15",
    "jquery" : "~1.12.2",
    "jquery-ui" : "~1.11.4",
    "requirejs-tpl-bower": "~0.1.0",
    "requirejs": "2.2.0",
    "velocity" : "git://github.com/julianshapiro/velocity",
    "backbone-localstorage" : "git://github.com/jeromegn/Backbone.localStorage",
    "marionette" : "~2.4.7"
  },

  "overrides" : {
    "bootstrap" : {
      "main": [
        "/**/*.less",
        "dist/fonts/glyphicons-halflings-regular.eot",
        "dist/fonts/glyphicons-halflings-regular.svg",
        "dist/fonts/glyphicons-halflings-regular.ttf",
        "dist/fonts/glyphicons-halflings-regular.woff"
      ],
      "dependencies": {}
    }
  },

  "install": {
      "path" : {
        "js"  : "src/main/webapp/application/js/libs",
        "eot" : "src/main/webapp/application/fonts",
        "svg" : "src/main/webapp/application/fonts",
        "ttf" : "src/main/webapp/application/fonts",
        "woff": "src/main/webapp/application/fonts",
        "css" : "src/main/webapp/application/css"
      }
  }

}
